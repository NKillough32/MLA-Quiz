{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/manifest.json",
            "dest": "/static/manifest.json"
        },
        {
            "src": "/sw.js",
            "dest": "/static/sw.js"
        },
        {
            "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg))",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production"
    }
}